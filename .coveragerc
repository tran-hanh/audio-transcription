[run]
source = backend,src
omit = 
    */tests/*
    */test_*.py
    */__pycache__/*
    */__init__.py
    */venv/*
    */.venv/*
    src/transcribe.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

